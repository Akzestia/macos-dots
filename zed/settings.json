{
  "disable_ai": true,
  "redact_private_values": true,
  "git_panel": {
    "dock": "right",
  },
  "icon_theme": "Catppuccin Latte",
  "features": {
    "edit_prediction_provider": "none",
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
  },
  "base_keymap": "VSCode",
  "theme": "Catppuccin Espresso (Blur) [Heavy]",
  "ui_font_size": 17,
  "buffer_font_size": 18,
  "file_finder": {
    "modal_max_width": "medium",
  },
  "buffer_font_family": "Cantarell",
  "vim_mode": true,
  "vim": {},
  "relative_line_numbers": "enabled",
  "tab_bar": {
    "show": true,
  },
  "scrollbar": {
    "show": "never",
  },
  "tabs": {
    "show_diagnostics": "errors",
  },
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware",
  },
  "centered_layout": {
    "left_padding": 0.15,
    "right_padding": 0.15,
  },
  "inlay_hints": {
    "enabled": true,
  },
  "lsp": {},
  "languages": {
    "TypeScript": {
      "inlay_hints": {
        "enabled": true,
        "show_parameter_hints": false,
        "show_other_hints": true,
        "show_type_hints": true,
      },
    },
  },
  "terminal": {
    "font_family": "Noto Sans Syriac",
    "env": {
      "EDITOR": "zed --wait",
    },
  },
  "file_types": {
    "Dockerfile": ["Dockerfile", "Dockerfile.*"],
    "JSON": ["json", "jsonc", "*.code-snippets"],
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    //
    "**/out",
    "**/dist",
    "**/.husky",
    "**/.turbo",
    "**/.vscode-test",
    "**/.vscode",
    "**/.next",
    "**/.storybook",
    "**/.tap",
    "**/.nyc_output",
    "**/report",
    "**/node_modules",
    //
    "**/external",
    "**.cache/",
    "**/build/",
    "**/certs/",
  ],
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "project_panel": {
    "button": true,
    "dock": "right",
    "git_status": true,
  },
  "outline_panel": {
    "dock": "right",
  },
  "collaboration_panel": {
    "dock": "left",
  },
  "notification_panel": {
    "dock": "left",
  },
  "theme_overrides": {
    "Catppuccin Espresso (Blur) [Heavy]": {
      "editor.background": "#00000000",
      "background.appearance": "transparent",
    },
  },
}
